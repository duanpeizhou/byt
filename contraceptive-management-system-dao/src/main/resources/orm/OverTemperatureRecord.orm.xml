<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<access>PROPERTY</access>
	<entity class="cn.zectec.contraceptive.management.system.model.OverTemperatureRecord">
		<table name="overtemperaturerecord"></table>
		<discriminator-value>overTemperatureRecord</discriminator-value>
		<attributes>
			<basic name="overTemperatureDate">
				<column name="overTemperatureDate"/>
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="overTemperature">
				<column name="overTemperature"/>
			</basic>
			<basic name="recoveryDate">
				<column name="recoveryDate"/>
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="recoveryTemperature">
				<column name="recoveryTemperature"/>
			</basic>
		</attributes>
	</entity>
</entity-mappings>
