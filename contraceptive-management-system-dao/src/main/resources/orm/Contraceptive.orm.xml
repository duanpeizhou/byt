<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<access>PROPERTY</access>
	<entity class="cn.zectec.contraceptive.management.system.model.Contraceptive">
		<table name="contraceptive"></table>
		<attributes>
			<basic name="name">
				<column name="name" />
			</basic>
			<basic name="Description">
				<column name="Description"/>
			</basic>
			<basic name="category">
				<column name="category"/>
			</basic>
			<basic name="type">
				<column name="type"/>
			</basic>
			<basic name="allowingNumber">
				<column name="allowingNumber"/>
			</basic>
			<basic name="measuringUnit">
				<column name="measuringUnit"/>
			</basic>
			<basic name="minAge">
				<column name="minAge"/>
			</basic>
			<basic name="maxAge">
				<column name="maxAge"/>
			</basic>
			<basic name="regionalLimit">
				<column name="regionalLimit"/>
			</basic>
			<basic name="sexLimit">
				<column name="sexLimit"/>
			</basic>
		</attributes>
	</entity>
</entity-mappings>
