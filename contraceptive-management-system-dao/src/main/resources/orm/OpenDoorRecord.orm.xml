<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<access>PROPERTY</access>
	<entity class="cn.zectec.contraceptive.management.system.model.OpenDoorRecord">
		<table name="opendoorrecord"></table>
		<discriminator-value>openDoorRecord</discriminator-value>
		<attributes>
			<basic name="openDoorDate">
				<column name="openDoorDate"/>
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="closeDoorDate">
				<column name="closeDoorDate"/>
				<temporal>TIMESTAMP</temporal>
			</basic>
		</attributes>
	</entity>
</entity-mappings>
