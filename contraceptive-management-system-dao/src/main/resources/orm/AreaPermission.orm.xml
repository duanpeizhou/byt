<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<access>PROPERTY</access>
	<entity class="cn.zectec.contraceptive.management.system.model.AreaPermission">
		<table name="areapermission"></table>
		<discriminator-value>areapermission</discriminator-value>
		<attributes>
			<basic name="level">
				<column name="level"/>
				<enumerated>STRING</enumerated>
			</basic>
			<many-to-one name="area">
				<join-column name="area_id"/>
			</many-to-one>
		</attributes>
	</entity>
</entity-mappings>
